<div class='biz-overview shadow'>
	<a href="http://fiddler.io" class='to-biz-show' 
		data-id='<%= business.id%>'><img src="<%= business.imageUrl()%>" width='75' height='50'></img><%= business.get('title') %></a>
	<p class='clamped clamped-2'>
		<span class='text'>
			<%= business.get('description') %>
			<span class='ellipsis'>&#133</span>
			<span class='fill'></span>
		</span>
	</p>
	<button class='subscribe-toggle btn btn-primary'
	data-business-id='<%= business.id %>'><%= button_val %></button>
	<p class='clamped clamped-2'>
		<span class='text'>
			<a href="<%= article.get('link')%>" target='_blank'><%= article.get('title')%></a>
			<span class='ellipsis'>&#133</span>
			<span class='fill'></span>
		</span>
	</p>
</div>
		
